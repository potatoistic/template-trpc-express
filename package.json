{
  "name": "template-trpc-express",
  "version": "0.0.1",
  "description": "",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "husky install",
    "start": "wireit",
    "dev": "wireit",
    "dev:ts": "wireit",
    "dev:js": "wireit",
    "build": "wireit",
    "lint": "wireit",
    "lint:tsc": "wireit",
    "test": "wireit",
    "clean": "wireit",
    "clean:deep": "wireit"
  },
  "wireit": {
    "start": {
      "command": "node dist/index.js",
      "dependencies": [
        "build"
      ]
    },
    "dev": {
      "command": "concurrently \"npm run dev:ts\" \"npm run dev:js\"",
      "dependencies": [
        "build"
      ]
    },
    "dev:ts": {
      "command": "tsup src/index.ts --watch"
    },
    "dev:js": {
      "command": "node --watch dist/index.js"
    },
    "build": {
      "command": "tsup src/**/*.ts --format cjs,esm --dts",
      "dependencies": [
        "clean"
      ]
    },
    "lint": {
      "command": "eslint --fix --color --max-warnings 3 --cache --cache-location .eslintcache .",
      "dependencies": [
        "lint:tsc"
      ]
    },
    "lint:tsc": {
      "command": "tsc"
    },
    "test": {
      "command": "echo \"Error: no test specified\" && exit 0",
      "dependencies": [
        "lint"
      ]
    },
    "clean": {
      "command": "rimraf dist"
    },
    "clean:deep": {
      "command": "rimraf node_modules .wireit",
      "dependencies": [
        "clean"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/potatoistic/template-trpc-express.git"
  },
  "keywords": [],
  "author": "dhruvsaxena1998",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/potatoistic/template-trpc-express/issues"
  },
  "homepage": "https://github.com/potatoistic/template-trpc-express#readme",
  "devDependencies": {
    "@commitlint/cli": "^17.4.3",
    "@commitlint/config-angular": "^17.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/node": "^18.13.0",
    "@typescript-eslint/eslint-plugin": "^5.52.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.34.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "rimraf": "^4.1.2",
    "tsup": "^6.6.3",
    "typescript": "^4.9.5",
    "wireit": "^0.9.5"
  },
  "packageManager": "npm@9.4.2",
  "dependencies": {
    "@trpc/server": "^10.11.1",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "helmet": "^6.0.1",
    "superjson": "^1.12.2",
    "zod": "^3.20.6"
  }
}
